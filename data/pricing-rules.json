{
  "basePrice": 2000,
  "rules": [
    {
      "id": "type-projet",
      "multipliers": {
        "site-vitrine": 1.0,
        "ecommerce": 2.5,
        "application-web": 3.5,
        "refonte": 1.5
      }
    },
    {
      "id": "nombre-pages",
      "multipliers": {
        "1-5": 1.0,
        "6-10": 1.5,
        "11-20": 2.0,
        "20+": 2.5
      }
    },
    {
      "id": "design",
      "multipliers": {
        "oui-complet": 0.8,
        "oui-partiel": 0.9,
        "non": 1.2
      }
    },
    {
      "id": "fonctionnalites",
      "multipliers": {
        "basique": 1.0,
        "intermediaire": 1.5,
        "avance": 2.5
      }
    },
    {
      "id": "delai",
      "multipliers": {
        "urgent": 1.3,
        "normal": 1.0,
        "flexible": 0.9
      }
    }
  ],
  "budgetRanges": {
    "moins-5000": { "min": 0, "max": 5000 },
    "5000-10000": { "min": 5000, "max": 10000 },
    "10000-25000": { "min": 10000, "max": 25000 },
    "25000+": { "min": 25000, "max": 100000 }
  }
}
